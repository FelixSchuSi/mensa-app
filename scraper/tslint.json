{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
        "no-console": false
    },
    "rules": {
        "trailing-comma": [
            false
        ],
        "no-console": false,
        "semicolon": [
            true,
            "always"
        ]
    },
    "rulesDirectory": []
}