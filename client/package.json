{
  "name": "client",
  "homepage": "https://felixschusi.github.io/mensa-app",
  "version": "1.0.0",
  "description": "task manager",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.dev.js",
    "start:tls": "webpack-dev-server --https --cert ./cert.pem --key ./key.pem --config ./webpack.dev.js",
    "build": "webpack --config ./webpack.prod.js --progress",
    "build:dev": "webpack --config ./webpack.dev.js --progress",
    "build:capacitor": "webpack --config ./webpack.capacitor.js --progress",
    "build:watch": "webpack --config ./webpack.capacitor.js --watch --progress",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "karma start",
    "e2e": "tsc -p e2e && jasmine JASMINE_CONFIG_PATH=e2e/jasmine.json",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "publish": "npm run deploy",
    "copyDist": "nodemon -e * --watch dist -x \"npx cap sync\"",
    "watch": "concurrently \"npm run copyDist android\" \"npm run build:watch\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@types/jasmine": "^3.5.14",
    "@types/node": "^14.11.10",
    "@types/selenium-webdriver": "^4.0.9",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "@webpack-cli/serve": "^1.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^6.3.0",
    "css-loader": "^5.0.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "file-loader": "^6.1.1",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^4.5.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine": "^3.6.2",
    "jasmine-core": "^3.6.0",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-webpack": "^5.0.0-alpha.2",
    "mini-css-extract-plugin": "^1.0.0",
    "nodemon": "^2.0.6",
    "postcss-loader": "^4.0.4",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.3",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "style-loader": "^2.0.0",
    "to-string-loader": "^1.1.6",
    "ts-loader": "^8.0.5",
    "typescript": "^4.0.3",
    "webpack": "4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "workbox-cacheable-response": "^5.1.4",
    "workbox-cli": "^5.1.4",
    "workbox-precaching": "^5.1.4",
    "workbox-routing": "^5.1.4",
    "workbox-strategies": "^5.1.4",
    "workbox-webpack-plugin": "^5.1.4"
  },
  "dependencies": {
    "@capacitor/android": "^2.4.2",
    "@capacitor/cli": "^2.4.2",
    "@capacitor/core": "^2.4.6",
    "@capacitor/ios": "^2.4.2",
    "@ionic/core": "^5.4.3",
    "@ionic/pwa-elements": "^3.0.2",
    "@open-wc/lit-helpers": "^0.3.11",
    "bootstrap": "^4.5.3",
    "lit-element": "^2.4.0",
    "localforage": "^1.9.0",
    "macro-carousel": "^1.0.0",
    "uuid": "^8.3.1"
  }
}
