{
  "name": "server",
  "version": "1.0.0",
  "description": "task manager server",
  "private": true,
  "main": "index.js",
  "scripts": {
    "prod": "tsc && node src/app mongodb prodDB",
    "dev": "tsc && node src/app mongodb devDB",
    "watch:prod": "tsc && nodemon -e ts --exec \"npm run prod\"",
    "watch:dev": "tsc && nodemon -e ts --exec \"npm run dev\"",
    "lint": "eslint . --ext .ts",
    "test": "tsc && nyc jasmine JASMINE_CONFIG_PATH=jasmine.json"
  },
  "nyc": {
    "exclude": [
      "src/**/*.spec.js"
    ],
    "reporter": [
      "text",
      "html"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.8",
    "@types/jasmine": "^3.5.14",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.5.33",
    "@types/node": "^14.11.10",
    "@types/pg": "^7.14.5",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jasmine": "^3.6.2",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.3",
    "nodemon": "^2.0.6",
    "pg": "^8.4.1",
    "uuid": "^8.3.1"
  }
}
